    background-image: url('https://cdn.shopify.com/s/files/1/1087/1054/t/2/assets/filter-down.png?4534834210000004863');
    background-repeat: no-repeat;
    background-position: 96% center;
    cursor: pointer;
    font-family: "futura-pt", sans-serif;
    border-radius: 0;
}

.coll-filter.coll-dropdown-open {
	background-image: url('https://cdn.shopify.com/s/files/1/1087/1054/t/2/assets/filter-up.png?9925170344614093451');
}

.coll-dropdown {
    position: absolute;
    width: calc(1070px * 0.25 - 30px);
    margin-left: -16px;
    background-color: #ef4315;
    margin-top: 7px;
    height: 0;
    overflow: hidden;
    -webkit-transition: height 500ms ease;
    -moz-transition: height 500ms ease;
    -o-transition: height 500ms ease;
    transition: height 500ms ease;
}

.coll-filter.coll-dropdown-open .coll-dropdown {
	height: initial;
}

.coll-dropdown-child {
	margin-bottom: 0;
}

.coll-dropdown-child a {
    padding: 4px 0 4px 16px;
    display: block;
    color: #fff;
}

.coll-dropdown-child a:hover {
	background-color: #d93d13;
}

